    function ajax_post(num,type){
        $.post('https://478bf9eb-d7b2-48ee-80fb-e89475807529.coding.io/index.php',{num:num,type:type},function(res){
            if(res !='no message' && res !='invalid request' ){
                var obj = JSON.parse(res);
                var str = "";
                for(var i = 0; i < obj.length; i ++) {
                    str +="<p class='row'><span class='col-sm-5'>"+obj[i].time+"</span><span class='col-sm-7'>"+obj[i].desc+"</span></p>";

                }
                $('#FEDEX-shipping').toggleClass('uk-hidden').html(str.replace(/\[America\/New_York\]/g, ""));
            }else{
                if(res == 'no message'){

                    ajax_post2(num,type);
                }else{
                    console.log(res);
                }


            }
        });
    }
